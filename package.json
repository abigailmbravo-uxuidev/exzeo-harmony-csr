{
  "name": "harmony-csr",
  "version": "0.9.1",
  "private": true,
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --coverage --watchAll=false",
    "test:watch": "react-scripts test",
    "lint": "./node_modules/.bin/eslint src --ext .jsx --ext .js",
    "cypress": "react-scripts cypress"
  },
  "dependencies": {
    "@exzeo/core-ui": "0.0.0-cd8674",
    "@exzeo/react-scripts": "2.5.0",
    "@fortawesome/fontawesome": "~1.0.0",
    "@fortawesome/react-fontawesome": "~0.0.14",
    "@uppy/core": "~1.7.0",
    "@uppy/react": "~1.4.1",
    "@uppy/xhr-upload": "~1.4.1",
    "auth0-js": "9.11.3",
    "bluebird": "^3.5.3",
    "classnames": "~2.2.5",
    "font-awesome": "~4.7.0",
    "history": "~4.6.2",
    "jwt-decode": "~2.2.0",
    "lodash": "^4.17.15",
    "moment": "~2.21.0",
    "moment-timezone": "^0.5.26",
    "prop-types": "~15.6.0",
    "query-string": "^6.8.3",
    "react": "^16.12.0",
    "react-app-polyfill": "^0.2.0",
    "react-bootstrap-table": "~4.3.1",
    "react-dom": "^16.12.0",
    "react-helmet": "~5.2.0",
    "react-modal": "~3.1.7",
    "react-number-format": "~3.0.3",
    "react-redux": "~5.0.6",
    "react-router-dom": "^5.0.0",
    "react-select": "~1.1.0",
    "react-tooltip": "~3.4.0",
    "redux": "~3.7.2",
    "redux-batched-actions": "~0.1.6",
    "redux-form": "7.3.x",
    "redux-thunk": "~2.2.0",
    "reselect": "~3.0.1",
    "validator": "~7.2.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.1.1",
    "@testing-library/react": "^9.4.0",
    "axios": "^0.19.0",
    "axios-mock-adapter": "~1.10.0",
    "cypress": "3.8.2",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "husky": "^2.7.0",
    "istanbul": "~0.4.5",
    "jest-dom": "^3.1.3",
    "jest-enzyme": "~4.0.1",
    "jest-teamcity-reporter": "^0.9.0",
    "lint-staged": "^8.2.1",
    "prettier": "^1.18.2",
    "redux-mock-store": "~1.3.0",
    "sinon": "~2.4.1"
  },
  "jest": {
    "testResultsProcessor": "jest-teamcity-reporter",
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/index.js",
      "!src/routes.js",
      "!src/state/reducers/initialState.js",
      "!src/setupTests.js",
      "!src/test-utils/**/*",
      "!src/components/Authentication.jsx",
      "!src/Auth.js"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 49
      }
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "./.eslintrc"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,css,scss,json,md}": [
      "prettier --single-quote --write",
      "git add"
    ],
    "cypress/**/*.js": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "ie 11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      "ie 11"
    ]
  }
}
