{
  "name": "harmony-csr",
  "version": "0.9.1",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --coverage --watchAll=false",
    "test:watch": "react-scripts test",
    "lint": "./node_modules/.bin/eslint src --ext .jsx --ext .js",
    "cypress": "react-scripts cypress"
  },
  "dependencies": {
    "@exzeo/core-ui": "0.8.7-alpha.3",
    "@exzeo/react-scripts": "^2.0.3",
    "@fortawesome/fontawesome": "~1.0.0",
    "@fortawesome/react-fontawesome": "~0.0.14",
    "@uppy/core": "^0.26.0",
    "@uppy/react": "^0.26.0",
    "@uppy/xhr-upload": "^0.26.0",
    "auth0-js": "~8.10.1",
    "bluebird": "^3.5.3",
    "classnames": "~2.2.5",
    "font-awesome": "~4.7.0",
    "history": "~4.6.2",
    "jwt-decode": "~2.2.0",
    "lodash": "~4.17.4",
    "moment": "~2.21.0",
    "moment-timezone": "0.5.x",
    "prop-types": "~15.6.0",
    "query-string": "^6.2.0",
    "react": "^16.8.4",
    "react-app-polyfill": "^0.2.0",
    "react-bootstrap-table": "~4.3.1",
    "react-dom": "^16.8.4",
    "react-helmet": "~5.2.0",
    "react-modal": "~3.1.7",
    "react-number-format": "~3.0.3",
    "react-redux": "~5.0.6",
    "react-router-dom": "^5.0.0",
    "react-select": "~1.1.0",
    "react-tooltip": "~3.4.0",
    "redux": "~3.7.2",
    "redux-batched-actions": "~0.1.6",
    "redux-form": "7.3.x",
    "redux-thunk": "~2.2.0",
    "reselect": "~3.0.1",
    "validator": "~7.2.0"
  },
  "devDependencies": {
    "axios-mock-adapter": "~1.10.0",
    "cypress": "3.2.x",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "husky": "^2.4.1",
    "istanbul": "~0.4.5",
    "jest-dom": "^3.1.3",
    "jest-enzyme": "~4.0.1",
    "lint-staged": "^8.2.1",
    "prettier": "^1.18.2",
    "react-testing-library": "^6.0.3",
    "redux-mock-store": "~1.3.0",
    "sinon": "~2.4.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/index.js",
      "!src/routes.js",
      "!src/modules/Policy/index.js",
      "!src/components/Authentication.jsx",
      "!src/setupTests.js",
      "!src/Auth.js"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 70
      }
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,css,scss,json,md}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 10",
    "not op_mini all"
  ]
}
