{
  "name": "harmony-csr",
  "version": "0.9.1",
  "private": true,
  "scripts": {
    "css:build": "node-sass src/sass --output-style 'compressed' -o src/css",
    "css:watch": "node-sass src/sass --output-style 'compressed' -o src/css --watch --recursive",
    "server": "./replace-dotenv.sh && exframe-web",
    "start": "react-scripts start",
    "start:dev": "npm run css:build && npm start",
    "build": "npm run css:build && react-scripts build",
    "test": "react-scripts test --env=jsdom --coverage",
    "test:watch": "react-scripts test --env=jsdom"
  },
  "dependencies": {
    "@exzeo/core-ui": "~0.3.1",
    "@fortawesome/fontawesome": "~1.0.0",
    "@fortawesome/react-fontawesome": "~0.0.14",
    "@uppy/core": "^0.26.0",
    "@uppy/react": "^0.26.0",
    "@uppy/xhr-upload": "^0.26.0",
    "auth0-js": "~8.10.1",
    "axios": "~0.15.2",
    "classnames": "~2.2.5",
    "exframe-web": "~1.0.0",
    "font-awesome": "~4.7.0",
    "history": "~4.6.2",
    "immutable": "~3.8.2",
    "jwt-decode": "~2.2.0",
    "lodash": "~4.17.4",
    "moment": "~2.21.0",
    "moment-timezone": "0.5.x",
    "node-sass": "~4.9.3",
    "npm-run-all": "~4.1.2",
    "prop-types": "~15.6.0",
    "react": "^16.4.2",
    "react-bootstrap-table": "~4.3.1",
    "react-cookie": "~2.1.2",
    "react-dom": "^16.4.2",
    "react-dropzone": "~4.2.3",
    "react-helmet": "~5.2.0",
    "react-modal": "~3.1.7",
    "react-number-format": "~3.0.3",
    "react-redux": "~5.0.6",
    "react-router": "~4.2.0",
    "react-router-dom": "~4.2.2",
    "react-scripts": "1.1.5",
    "react-select": "~1.1.0",
    "react-tooltip": "~3.4.0",
    "redux": "~3.7.2",
    "redux-batched-actions": "~0.1.6",
    "redux-form": "7.3.x",
    "redux-thunk": "~2.2.0",
    "reselect": "~3.0.1",
    "validator": "~7.2.0"
  },
  "devDependencies": {
    "axios-mock-adapter": "~1.10.0",
    "babel-jest": "~19.0.0",
    "babel-plugin-transform-decorators-legacy": "~1.3.4",
    "babel-polyfill": "~6.23.0",
    "babel-preset-airbnb": "~2.4.0",
    "babel-preset-es2015": "~6.24.1",
    "babel-preset-react": "~6.24.1",
    "babel-preset-stage-0": "~6.24.1",
    "cross-env": "~5.1.5",
    "enzyme": "~3.2.0",
    "enzyme-adapter-react-16": "~1.1.0",
    "eslint": "~4.13.1",
    "eslint-config-airbnb": "~16.1.0",
    "eslint-config-react": "~1.1.7",
    "eslint-plugin-import": "~2.8.0",
    "eslint-plugin-jsx-a11y": "~6.0.3",
    "eslint-plugin-react": "~7.6.1",
    "eslint-plugin-you-dont-need-lodash-underscore": "~6.3.1",
    "istanbul": "~0.4.5",
    "jest-enzyme": "~4.0.1",
    "react-test-renderer": "~16.2.0",
    "redux-mock-store": "~1.3.0",
    "sinon": "~2.4.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/index.js",
      "!src/routes.js",
      "!src/modules/Policy/index.js",
      "!src/components/Authentication.jsx",
      "!src/setupTests.js",
      "!src/Auth.js"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 80
      }
    }
  }
}
