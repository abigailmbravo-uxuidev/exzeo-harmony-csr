    "@exzeo/core-ui": "0.1.x",
    "auth0-js": "~8.10.1",
    "axios": "~0.15.2",
    "classnames": "~2.2.5",
    "eslint": "~4.13.1",
    "eslint-config-airbnb": "~16.1.0",
    "eslint-config-react": "~1.1.7",
    "exframe-web": "^1.0.0",
    "font-awesome": "~4.7.0",
    "history": "~4.6.2",
    "immutable": "~3.8.2",
    "jwt-decode": "~2.2.0",
    "lodash": "~4.17.4",
    "moment": "~2.21.0",
    "moment-timezone": "~0.5.14",
    "node-sass": "~4.7.2",
    "prop-types": "~15.6.0",
    "react": "~16.2.0",
    "react-bootstrap-table": "~4.0.3",
    "react-cookie": "~2.1.2",
    "react-dom": "~16.2.0",
    "react-dropzone": "~4.2.3",
    "react-helmet": "~5.2.0",
    "react-modal": "~3.1.7",
    "react-number-format": "~3.0.3",
    "react-redux": "~5.0.6",
    "react-router": "~4.2.0",
    "react-router-dom": "~4.2.2",
    "react-scripts": "~1.0.17",
    "react-select": "~1.1.0",
    "react-tooltip": "~3.4.0",
    "redux": "~3.7.2",
    "redux-batched-actions": "~0.1.6",
    "redux-form": "7.3.x",
    "redux-persist": "~4.10.2",
    "redux-thunk": "~2.2.0",
    "uppy": "^0.23.1",
    "validator": "~7.2.0"
  },
  "devDependencies": {
    "axios-mock-adapter": "~1.10.0",
    "babel-jest": "~19.0.0",
    "babel-plugin-transform-decorators-legacy": "~1.3.4",
    "babel-polyfill": "~6.23.0",
    "babel-preset-airbnb": "~2.4.0",
    "babel-preset-es2015": "~6.24.1",
    "babel-preset-react": "~6.24.1",
    "babel-preset-stage-0": "~6.24.1",
    "enzyme": "~3.2.0",
    "enzyme-adapter-react-16": "~1.1.0",
    "eslint": "~4.13.1",
    "eslint-plugin-import": "~2.8.0",
    "eslint-plugin-jsx-a11y": "~6.0.3",
    "eslint-plugin-react": "~7.6.1",
    "eslint-plugin-you-dont-need-lodash-underscore": "~6.3.1",
    "istanbul": "~0.4.5",
    "jest-enzyme": "~4.0.1",
    "react-test-renderer": "~16.2.0",
    "redux-mock-store": "~1.3.0",
    "sinon": "~2.4.1"
  },
  "scripts": {
    "build-css": "node-sass src/sass --output-style 'compressed' -o src/css",
    "watch-css": "node-sass src/sass --output-style 'compressed' -o src/css --watch --recursive",
    "dev": "npm run build-css && react-scripts start",
    "start": "react-scripts start",
    "server": "./replace-dotenv.sh && exframe-web",
    "build": "npm run build-css && react-scripts build",
    "cover": "npm test -- --coverage",
    "test": "npm run build && react-scripts test --env=jsdom --coverage",
    "eject": "react-scripts eject",
    "link-core": "npm link @exzeo/core-ui",
    "unlink-core": "npm unlink @exzeo/core-ui",
    "test:watch": "react-scripts test --env=jsdom"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/index.js",
      "!src/routes.js",
      "!src/setupTests.js",
      "!src/Auth.js"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 80
      }
    }
  }
}
